{
    "name": "allay/base",
    "type": "library",
    "description": "Allay's base package, which offers a bare-bones website admin package, built on Laravel and AdminLTE theme",
    "keywords": [
        "allay",
        "base"
    ],
    "homepage": "https://github.com/laravel-allay/Base",
    "license": "MIT",
    "authors": [
        {
            "name": "Zachary Schuessler",
            "email": "zlschuessler@gmail.com",
            "homepage": "https://github.com/laravel-allay/Base",
            "role": "Package Maintainer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "almasaeed2010/AdminLTE" : "2.3.*",
        "laravel/framework": "5.3.*",
        "prologue/alerts": "0.4.*",
        "jenssegers/date": "^3.2",
        "jrean/laravel-user-verification": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Allay\\Base\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Allay\\Base\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
